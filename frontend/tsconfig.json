{
  "include": ["src", "ui", "gnomad-browser-toolkit"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "jsx": "react-jsx",
    "outDir": "dist",
    "module": "ES2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@axaou/ui": ["ui/axaou-ui/src"],
      "@axaou/ui/*": ["ui/axaou-ui/src/*"],
      "@axaou/types": ["ui/types/src"],
      "@axaou/types/*": ["ui/types/src/*"],
      "@gnomad/classification-selector": ["gnomad-browser-toolkit/packages/classification-selector/src"],
      "@gnomad/identifiers": ["gnomad-browser-toolkit/packages/identifiers/src"],
      "@gnomad/manhattan-plot": ["gnomad-browser-toolkit/packages/manhattan-plot/src"],
      "@gnomad/markdown-loader": ["gnomad-browser-toolkit/packages/markdown-loader/src"],
      "@gnomad/qq-plot": ["gnomad-browser-toolkit/packages/qq-plot/src"],
      "@gnomad/region-viewer": ["gnomad-browser-toolkit/packages/region-viewer/src"],
      "@gnomad/track-transcripts": ["gnomad-browser-toolkit/packages/track-transcripts/src"],
      "@gnomad/track-variants": ["gnomad-browser-toolkit/packages/track-variants/src"],
      "@gnomad/ui": ["gnomad-browser-toolkit/packages/ui/src"]
    }
  }
}
